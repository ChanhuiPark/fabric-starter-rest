              {name: 'ORDERER_DOMAIN',value: 'osn-${ORG}.${DOMAIN}'},
              {name: 'ORDERER_PORTS', value: '7150,7250,7350'}
            ],
            steps: [
              {
                step: "1",
                task: 'startRaftOrderingService3Nodes',
                auto: true,
              },
              {
                step: "2",
                scenario: 'inviteOrdererToRAFTCluster',
                auto: true,
              },
            ]
          },

