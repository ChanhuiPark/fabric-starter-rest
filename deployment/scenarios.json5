          joinRaftOrderingService: {
            name: 'Prepare my RAFT orderer and Join existing RAFT OSN',
            params: [
              {name: 'TARGET_ORDERER', value: 'raft0.osn-org1.${DOMAIN}'},
              {name: 'MY_ORDERER_NAME', value: 'raft4'},
                     {name: 'MY_ORDERER_DOMAIN',value: 'osn-${ORG}.${DOMAIN}'},
                     {name: 'MY_ORDERER_PORT', value: '7150'},
                    {name: 'apiPort', value: '4000'}, {name: 'task', value: 'AddOrdererToRaftOrderingService', visible: 'false'}
            ],
            steps: [
              {
                task: "PrepareRaft1Node",
                params: {ordererName: "${MY_ORDERER_NAME}", ordererDomain: "${MY_ORDERER_DOMAIN}"}
              },
              {
                task: 'PassTaskToOtherParty',
                params: [{name: 'task', value: "AddOrdererToRaftOrderingService", valueIsATask: true}],
                auto: true,
              },
              {
                task: "StartRaft1Node",
                params: {ordererName: "${MY_ORDERER_NAME}", ordererDomain: "${MY_ORDERER_NAME}"}
              }
            ]
          },
