      name: "Join me to existing channel",
      params: [{name:'channel', value: "common"}, {name:'orgId', value:"${ORG}"}, {name:"orgIp", value:"${MY_IP}"}, {name: 'apiPort', value: '4000'}, {name: 'task', value: 'AddOrgToChannel', visible: 'false'}],
      steps: [
        {
          task: 'PassTaskToOtherParty',
          params: [{name: 'task', value: "AddOrgToChannel", valueIsATask: true}],
          auto: true,
        },
        {
          task: 'WaitForOtherPartyTaskCompleted',
          params: {
            entryPointCallback: "${MY_IP}",
          },
          auto: true,
        },
        {
          task: 'JoinChannel',
          params: {
